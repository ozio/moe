{
  "source": {
    "include": [
      "package.json",
      "README.md"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|dist|gulp|tests/)"
  },
  "plugins": [
    "node_modules/jsdoc-strip-async-await",
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/oz-jsdoc"
  }
}
